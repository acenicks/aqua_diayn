<launch>
  <arg name="project_path" value="$(env PROJECT_PATH)"/>
  <arg name="skills_file" default="$(arg project_path)/data/seed_1/itr_0.pkl" />

    <include file="$(find aqua_gazebo)/launch/aqua.launch"></include>
    <include file="$(find aqua_diayn)/launch/basic.launch"></include>

    <node
      name="visualize_skills"
      pkg="aqua_diayn"
      type="visualize_skills.py"
      args="--file=$(arg skills_file)"
      output="screen">
    </node>

</launch>
